   "source": [
    "import numpy as np\n",
    "def normalize(x):\n",
    "    min = np.min(x)\n",
    "    max = np.max(x)\n",
    "    range = max - min\n",
    "    return [(a - min) / range for a in x]\n",
    "x = [70000, 211111, 111113, 1511111]\n",
    "normalizedX = normalize(x)\n",
    "print(normalizedX)"
   ]
  }
